za-meta-plugins-meta-cmd-help-handler() {
    +zi::message "—— {pre}meta {meta}--help{data}/{meta}-h" \
            "{rst}{nl}{nl}" \
            \
            "{msg2}Description:{rst}{nl}" \
            "{meta}--help{data}/{meta}-h{ehi}:{rst}{msg}       this message{rst}{nl}"
    return 0
}

